[
  {
    "type": "click",
    "target": "Chrome Profile Picker",
    "button": "left",
    "element_selector": {
      "name": "Who's using Chrome?",
      "control_type": "Window",
      "class_name": "Chrome_WidgetWin_1",
      "process_name": "chrome"
    },
    "coordinate_selector": {
      "coords": {
        "x": 913,
        "y": 1477
      },
      "bbox": {
        "left": 900,
        "top": 1460,
        "right": 930,
        "bottom": 1490
      }
    },
    "description": "Click on Chrome profile picker window"
  },
  {
    "type": "load",
    "description": "Wait for profile picker to load",
    "condition": {
      "type": "uia.exists",
      "selector": {
        "name": "ProfilePickerView",
        "control_type": "Pane",
        "process_name": "chrome"
      }
    },
    "timeout": 10
  },
  {
    "type": "click",
    "target": "Add Profile Button",
    "button": "left",
    "element_selector": {
      "name": "Add",
      "control_type": "Button",
      "class_name": "profile-item",
      "process_name": "chrome"
    },
    "coordinate_selector": {
      "coords": {
        "x": 1227,
        "y": 751
      },
      "bbox": {
        "left": 1200,
        "top": 730,
        "right": 1250,
        "bottom": 770
      }
    },
    "description": "Click the Add profile button"
  },
  {
    "type": "load",
    "description": "Wait for new profile creation dialog",
    "condition": {
      "type": "uia.exists",
      "selector": {
        "name": "Create a profile",
        "control_type": "Window",
        "process_name": "chrome"
      }
    },
    "timeout": 15
  },
  {
    "type": "type_text",
    "text": "Test Profile",
    "description": "Enter profile name"
  },
  {
    "type": "click",
    "target": "Create Profile Button",
    "button": "left",
    "element_selector": {
      "name": "Create",
      "control_type": "Button",
      "process_name": "chrome"
    },
    "coordinate_selector": {
      "coords": {
        "x": 1872,
        "y": 239
      },
      "bbox": {
        "left": 1850,
        "top": 220,
        "right": 1890,
        "bottom": 260
      }
    },
    "description": "Click Create profile button"
  },
  {
    "type": "wait_user_input",
    "description": "Wait for user to complete profile setup",
    "condition": {
      "type": "url.contains",
      "value": "chrome://welcome"
    },
    "timeout": 60
  }
]
